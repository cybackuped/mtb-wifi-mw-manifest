<middleware version="2.0">
  <middleware>
    <name>connectivity-utilities</name>
    <id>connectivity-utilities</id>
    <uri>https://github.com/cymodif/connectivity-utilities</uri>
    <desc>The connectivity utilities library is a collection of general purpose middleware utilities. Several connectivity middleware libraries shall depend on this utilities library. Currently, the following utilities are included: JSON parser, linked list, string utilities, network helpers and logging functions, and connectivity middleware error codes.</desc>
    <category>WiFi Middleware libraries</category>
    <req_capabilities>psoc6 cyw43xxx wifi</req_capabilities>
    <versions>
      <version flow_version="1.0,2.0">
        <num>Latest 3.X release</num>
        <commit>latest-v3.X</commit>
        <desc>Latest 3.X release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>3.0.1 release</num>
        <commit>release-v3.0.1</commit>
        <desc>3.0.1 release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>3.0.0 release</num>
        <commit>release-v3.0.0</commit>
        <desc>3.0.0 release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>Latest 2.X release</num>
        <commit>latest-v2.X</commit>
        <desc>Latest 2.X release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>2.1.1 release</num>
        <commit>release-v2.1.1</commit>
        <desc>2.1.1 release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>2.1.0 release</num>
        <commit>release-v2.1.0</commit>
        <desc>2.1.0 release</desc>
      </version>
      <version flow_version="1.0,2.0">
        <num>2.0.0 release</num>
        <commit>release-v2.0.0</commit>
        <desc>2.0.0 release</desc>
      </version>
    </versions>
  </middleware>
</middleware>
